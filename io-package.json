{
  "common": {
    "name": "iobroker.ksh",
    "version": "0.0.2,
    "news": {
      "0.0.1": {
        "de": "Startversion erstellt."
      },
      "0.0.2": {
        "de": "kleinere Anpassungen der Adminview durchgeführt.\n Lizenz Datei hinzugefügt um die Adapteransicht anzupassen.\n ersten Test der Versionsanzeige beim Admin eingebaut."
      }
    },
    "title": "KSH Adapter",
    "titleLang": {
      "de": "KiliksSmartHomeAdapter"
    },
    "desc": {
      "de": "Der Adapter ermöglicht die Kommunikation von meinen KSH Geräten mit dem IoB"
    },
    "authors": [
      "kilik <kilik133@gmail.com>"
    ],
    "docs": {
      "de": "README.md"
    },
    "license": "MIT",
    "platform": "Javascript/Node.js",
    "mode": "daemon",
    "compact": true,
    "messagebox": true,
    "readme": "https://github.com/Kilik133/ioBroker.ksh/blob/master/README.md",
    "loglevel": "info",
    "icon": "ksh.png",
    "materialize": true,
    "keywords": [
      "iobroker",
      "kst"
    ],
   "extIcon": "https://github.com/Kilik133/ioBroker.ksh/blob/master/admin/ksh.png",
   "type": "protocols",
   "plugins": {
     "sentry": {
       "dsn": "https://40b55da1fb50459eab397fe8291e75fe@sentry.iobroker.net/91"
     }
   },
    "dependencies": [
      {
        "js-controller": ">=2.0.0"
      }
    ],
    "tier": 2,
    "connectionType": "local",
    "dataSource": "push"
  },
  "native": {
    "type": "server",
    "bind": "0.0.0.0",
    "port": 1888,
    "ssl": false,
    "rejectUnauthorized": true,
    "user": "KST_User",
    "pass": "1234",
    "url": "localhost",
    "patterns": "#",
    "onchange": true,
    "publishAllOnStart": true,
    "debug": false,
    "publish": "kst.0.*",
    "prefix": "",
    "forceCleanSession": "no",
    "sendAckToo": false,
    "sendStateObject": false,
    "webSocket": false,
    "maxTopicLength": 100,
    "publishOnSubscribe": true,
    "extraSet": false,
    "sendOnStartInterval": 2000,
    "sendInterval": 10,
    "keepalive": 1000,
    "reconnectPeriod": 10,
    "connectTimeout": 30,
    "persistent": false,
    "defaultQoS": 0,
    "retain": false,
    "retransmitInterval": 2000,
    "retransmitCount": 10,
    "storeClientsTime": 1440
  },
  "objects": [],
  "instanceObjects": [
    {
      "_id": "info",
      "type": "channel",
      "common": {
        "name": "Informationen"
      },
      "native": {}
    },
    {
      "_id": "info.verbindungen",
      "type": "state",
      "common": {
        "role": "indicator.connected",
        "name": "Liste der Verbundenen Geräte",
        "type": "boolean",
        "read": true,
        "write": false,
        "def": false
      },
      "native": {}
    }
  ]
}
