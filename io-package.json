{
  "common": {
    "name": "iobroker.ksh",
    "version": "0.0.4",
    "news": {
      "0.0.4": {
        "de": "Problembehebung > Issue #3."
      }
    },
    "title": "KSH Adapter",
    "titleLang": {
      "de": "KiliksSmartHomeAdapter"
    },
    "desc": {
      "de": "Der Adapter ermöglicht die Kommunikation von meinen KSH Geräten mit dem IoB"
    },
    "authors": [
      "kilik <kilik133@gmail.com>"
    ],
    "docs": {
      "de": "README.md"
    },
    "license": "MIT",
    "platform": "Javascript/Node.js",
    "mode": "daemon",
    "compact": true,
    "messagebox": true,
    "readme": "https://github.com/Kilik133/ioBroker.ksh/blob/master/README.md",
    "loglevel": "info",
    "icon": "ksh.png",
    "materialize": true,
    "keywords": [
      "ksh",
      "KiliksSmartHome"
    ],
    "extIcon": "https://github.com/Kilik133/ioBroker.ksh/blob/master/admin/ksh.png",
    "type": "protocols",
    "plugins": {
      "sentry": {
        "dsn": "https://40b55da1fb50459eab397fe8291e75fe@sentry.iobroker.net/91"
      }
    },
    "dependencies": [
      {
        "js-controller": ">=3.0.0"
      }
    ],
    "globalDependencies": [
      {
        "admin": ">=5.4.3"
      }
    ],
    "tier": 2,
    "connectionType": "local",
    "dataSource": "push",
    "adminUI": {
      "config": "json"
    },
    "messages": [
      {
        "condition": {
          "operand": "and",
          "rules": [
            "oldVersion<0.0.3",
            "newVersion>=0.0.3"
          ]
        },
        "title": {
          "de": "TESTMESSAGE TITLE!"
        },
        "text": {
          "de": "TESTMESSAGE TEXT!"
        },
        "level": "warn",
        "buttons": [
          "agree",
          "cancel"
        ]
      },
      {
        "condition": {
          "operand": "and",
          "rules": [
            "oldVersion<0.0.5",
            "newVersion>=0.0.6"
          ]
        },
        "title": {
          "de": "WAS DAS HIER?"
        },
        "text": {
          "de": "WAS DAS HIER TEXT"
        },
        "level": "warn",
        "buttons": [
          "agree",
          "cancel"
        ]
      }
    ]
  },
  "encryptedNative": [
    "pass"
  ],
  "protectedNative": [
    "pass"
  ],
  "native": {
    "type": "server",
    "clientId": "",
    "bind": "0.0.0.0",
    "port": 1888,
    "ssl": false,
    "rejectUnauthorized": true,
    "user": "KST_User",
    "pass": "1234",
    "url": "localhost",
    "patterns": "#",
    "onchange": true,
    "publishAllOnStart": true,
    "debug": false,
    "publish": "ksh.0.*",
    "certPublic": "",
    "certPrivate": "",
    "certChained": "",
    "prefix": "",
    "forceCleanSession": "no",
    "sendAckToo": false,
    "sendStateObject": false,
    "webSocket": false,
    "maxTopicLength": 100,
    "publishOnSubscribe": true,
    "extraSet": false,
    "sendOnStartInterval": 2000,
    "sendInterval": 10,
    "keepalive": 1000,
    "reconnectPeriod": 10,
    "connectTimeout": 30,
    "persistent": false,
    "defaultQoS": 0,
    "retain": false,
    "retransmitInterval": 2000,
    "retransmitCount": 10,
    "storeClientsTime": 1440,
    "removePrefix": "",
    "ignoreNewObjects": false,
    "noRetain": false,
    "allBinaries": false
  },
  "objects": [],
  "instanceObjects": [
    {
      "_id": "info",
      "type": "channel",
      "common": {
        "name": "Information"
      },
      "native": {}
    },
    {
      "_id": "info.connection",
      "type": "state",
      "common": {
        "role": "indicator.connected",
        "name": "connected client for MQTT",
        "type": "boolean",
        "read": true,
        "write": false,
        "def": false
      },
      "native": {}
    }
  ]
}
